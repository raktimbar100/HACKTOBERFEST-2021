#include<stdio.h>
#include<string.h>
int main()
{
char a[50],b[50],temp[50],p[50];
int index,i,j=0,c=0,l,r,k=0,m=0;
gets(a);
for(i=0;i<=strlen(a)-1;i++){
p[j]=a[i];
j++;
}
j=0;
printf("enter the l=1 for left shift\n");
scanf("%d",&l);
while(l==1){
printf("enter the index\n");
scanf("%d",&index);
for(i=index;i<=strlen(a)-1;i++){
temp[j]=a[i];
j++;
c++;
}
for(i=index-1;i>=0;i--){
a[i+c]=a[i];
}
j=0;
for(i=0;i<c;i++){
a[i]=temp[j];
j++;
}
j=0;

c=0;
puts(a);
b[k]=a[0];
k++;
printf("enter the l=1 for left shift\n");
scanf("%d",&l);
}
printf("enter the r=1 for right shift\n");
scanf("%d",&r);
while(r==1){
printf("enter the index\n");
scanf("%d",&index);
j=0;
c=0;
for(i=0;i<=index;i++){
temp[j]=a[i];
j++;
c++;
}
for(i=index+1;i<=strlen(a)-1;i++){
a[i-c]=a[i];
}
j=0;
for(i=strlen(a)-c;i<=strlen(a)-1;i++){
a[i]=temp[j];
j++;
}
j=0;
c=0;
puts(a);
b[k]=a[0];
k++;

printf("enter the r=1 for right shift\n");
scanf("%d",&r);
}
for(k=0;k<=strlen(b)-1;k++){
for(j=0;j<=strlen(p)-1;j++){
if(b[k]==p[j]){
m++;
p[j]=-78;
}
}
}
puts(b);
if(m==strlen(p)){
printf("YES\n");
}
if(m!=strlen(p)){
printf("NO\n");
}
}
